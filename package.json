{
  "name": "place-my-order",
  "version": "0.10.0",
  "description": "",
  "homepage": "",
  "author": {
    "name": "",
    "email": "",
    "url": ""
  },
  "scripts": {
    "api": "place-my-order-api --port 7070",
    "test": "testee src/test.html --browsers firefox --reporter Spec",
    "start": "done-serve --proxy http://localhost:7070 --port 8080",
    "develop": "done-serve --develop --proxy http://localhost:7070 --port 8080",
    "document": "documentjs",
    "build": "node build"
  },
  "engines": {
    "node": "5.8"
  },
  "main": "production.html",
  "files": [
    "src"
  ],
  "keywords": [],
  "steal": {
    "main": "place-my-order/index.stache!done-autorender",
    "directories": {
      "lib": "src"
    },
    "configDependencies": [
      "live-reload",
      "node_modules/can-zone/register"
    ],
    "envs": {
      "server-production": {
        "renderingBaseURL": "https://place-my-order.firebaseapp.com/"
      }
    },
    "plugins": [
      "steal-stache",
      "steal-less",
      "done-component"
    ]
  },
  "dependencies": {
    "bit-tabs": "^1.0.0-alpha.0",
    "can-component": "^3.0.4",
    "can-connect": "^1.0.21",
    "can-define": "^1.0.10",
    "can-route": "^3.0.6",
    "can-route-pushstate": "^3.0.1",
    "can-stache": "^3.0.16",
    "can-view-autorender": "^3.0.3",
    "can-zone": "^0.6.0",
    "done-serve": "^1.0.0-alpha.0",
    "done-autorender": "^1.0.0-alpha.0",
    "done-component": "^0.6.0-pre.4",
    "done-css": "^3.0.0-alpha.0",
    "generator-donejs": "^1.0.0-alpha.0",
    "place-my-order-api": "^0.4.4",
    "place-my-order-assets": "^0.1.7",
    "steal": "^1.0.5",
    "steal-less": "^1.0.2",
    "steal-stache": "^3.0.4",
    "steal-platform": "0.0.4",
    "steal-socket.io": "^2.0.3"
  },
  "devDependencies": {
    "can-fixture": "^1.0.11",
    "documentjs": "^0.4.4",
    "donejs-deploy": "^0.4.2",
    "funcunit": "^3.1.0",
    "steal-cordova": "^0.1.3",
    "steal-nw": "^0.1.4",
    "steal-tools": "^1.0.1",
    "steal-qunit": "^1.0.0",
    "testee": "^0.3.0"
  },
  "window": {
    "width": 1000,
    "height": 600,
    "toolbar": false
  },
  "donejs": {
    "deploy": {
      "root": "dist",
      "services": {
        "production": {
          "type": "firebase",
          "config": {
            "firebase": "place-my-order",
            "public": "./dist",
            "headers": [
              {
                "source": "/**",
                "headers": [
                  {
                    "key": "Access-Control-Allow-Origin",
                    "value": "*"
                  }
                ]
              }
            ]
          }
        }
      }
    }
  }
}
