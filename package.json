      "lib": "src"
    },
    "configDependencies": [
      "live-reload",
      "node_modules/can-zone/register"
    ],
    "envs": {
      "server-production": {
        "renderingBaseURL": "https://place-my-order.firebaseapp.com/"
      }
    },
    "plugins": [
      "done-css",
      "done-component",
      "steal-less",
      "steal-stache"
    ],
    "serviceBaseURL": ""
  },
  "dependencies": {
    "bit-tabs": "^1.0.0-alpha.0",
    "can-component": "^3.0.4",
    "can-connect": "^1.0.21",
    "can-define": "^1.0.10",
    "can-route": "^3.0.6",
    "can-route-pushstate": "^3.0.1",
    "can-set": "^1.0.3",
    "can-stache": "^3.0.16",
    "can-view-autorender": "^3.0.3",
    "can-zone": "^0.6.0",
