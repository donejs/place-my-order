{
  "name": "place-my-order",
  "version": "0.2.0",
  "description": "The place-my-order frontend, built with DoneJS",
  "main": "app.html",
  "repository": "git@github.com:donejs/place-my-order.git",
  "scripts": {
    "document": "documentjs",
    "start": "node .server.js",
    "test": "testee test.html --browsers firefox --reporter Spec",
    "live-reload": "steal-tools live-reload",
    "build": "node build",
    "build:cordova": "node build cordova",
    "build:nw": "node build nw"
  },
  "author": "Bitovi",
  "license": "MIT",
  "dependencies": {
    "bit-tabs": "^0.2.0",
    "bootstrap": "^3.3.5",
    "can": "^2.3.0-beta.6",
    "can-connect": "~0.3.1",
    "can-ssr": "^0.9.1",
    "d3": "3.5.5",
    "done-autorender": "^0.4.2",
    "done-component": "^0.3.0",
    "done-css": "~1.1.13",
    "es6-promise": "^2.2.0",
    "jquery": "^1.11.3",
    "place-my-order-api": "^0.4.2",
    "place-my-order-assets": "^0.1.6",
    "socket.io-client": "^1.3.5",
    "steal": "^0.12.0-pre.7",
    "steal-platform": "0.0.4"
  },
  "devDependencies": {
    "documentjs": "^0.3.3",
    "donejs-deploy": "^0.1.6",
    "funcunit": "^3.0.0",
    "grunt": "^0.4.4",
    "grunt-cli": "^0.1.13",
    "steal-cordova": "0.1.1",
    "steal-nw": "0.1.3",
    "steal-qunit": "^0.1.1",
    "steal-tools": "^0.12.0-pre.4",
    "testee": "^0.2.0"
  },
  "donejs": {
    "deploy": {
      "root": "dist",
      "services": {
        "production": {
          "type": "divshot",
          "environment": "production",
          "config": {
            "name": "place-my-order",
            "headers": {
              "/**": {
                "Access-Control-Allow-Origin": "*"
              }
            }
          }
        }
      }
    }
  },
  "system": {
    "main": "place-my-order/index.stache!done-autorender",
    "directories": {
      "lib": "src"
    },
    "transpiler": "babel",
    "envs": {
      "production": {
        "baseURL": "https://place-my-order.divshot.io/"
      },
      "server-development": {
        "map": {
          "socket.io-client/socket.io": "@empty"
        }
      },
      "server-production": {
        "map": {
          "socket.io-client/socket.io": "@empty"
        }
      }
    },
    "meta": {
      "socket.io-client/socket.io": {
        "format": "global"
      }
    },
    "map": {
      "socket.io-client": "socket.io-client/socket.io"
    },
    "configDependencies": [
      "live-reload"
    ],
    "npmIgnore": [
      "steal-cordova",
      "steal-nw",
      "documentjs",
      "testee",
      "place-my-order-api",
      "donejs-deploy",
      "testee"
    ]
  },
  "window": {
    "width": 1060,
    "height": 625,
    "toolbar": false
  }
}
